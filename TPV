#Trabajo practico V UNLU

#Problema 1
def suma(x, y):
    print(x + y)

#Problema 2
def sumaret(x, y):
    return x + y

#Problema 3
def longitudstr(string):
    return len(string)

#Problema 4
def potencia(base, exponente):
    return base ** exponente

#Problema 5
def mayus(string):
    return string.upper()

#Problema 6



#Problema 7
def comparacion(nombre1, nombre2):
    return len(nombre1) > len(nombre2)
